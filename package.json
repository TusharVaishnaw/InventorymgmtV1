{
  "name": "inventory-management-system",
  "version": "1.0.0",
"scripts": {
  "build": "cd client && npm install && CI=false npm run build",
  "start": "npm run build && npm install && cd server && npm install && node server.js",
  "dev": "concurrently \"npm run server\" \"npm run client\"",
  "server": "cd server && npm run dev",
  "client": "cd client && npm start",
  "vercel-build": "cd client && npm install && npm run build"
},
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.1"
  },
  "engines": {
  "node": "20.x"
}
}